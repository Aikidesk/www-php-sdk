{
  "name": "aikidesk/www-php-sdk",
  "type": "library",
  "description": "PHP library for Aikidesk WWW API",
  "keywords": [
    "aikidesk",
    "api"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Dimitar Rekinov",
      "email": "d.rekinov@aikidesk.com"
    }
  ],
  "require": {
    "php": ">=7.4"
  },
  "require-dev": {
    "symfony/var-dumper": "~3|~4|~5|~6|~7",
    "guzzlehttp/guzzle": "~6|~7",
    "resty/resty": "@stable",
    "php-curl-class/php-curl-class": "~9"
  },
  "suggest": {
    "guzzlehttp/guzzle": "Guzzle HTTP Client v6 (with/out curl)",
    "resty/resty": "Resty v0.6 (without curl)",
    "php-curl-class/php-curl-class": "PHP Curl Class v4 (with curl)"
  },
  "autoload": {
    "psr-4": {
      "Aikidesk\\SDK\\WWW\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "preferred-install": "dist",
    "optimize-autoloader": true
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
